<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
	
	<bean id="egovKabAdmExcelView" class="egovframework.cms.com.excel.EgovKabAdmExcelView"/>
	<bean id="egovKabAdmStatExcelView" class="egovframework.cms.com.excel.EgovKabAdmStatExcelView"/>
	<bean id="beanNameResolver" class="org.springframework.web.servlet.view.BeanNameViewResolver" p:order="0" />
	<bean class="org.springframework.web.servlet.handler.SimpleMappingExceptionResolver">
	    <property name="order" value="0"/>
	    <property name="defaultErrorView" value="error"/>
	    <property name="exceptionMappings">
	        <props>
	               <prop key="RuntimeException">error</prop>
	        </props>
	    </property>
	</bean>
	
</beans>
